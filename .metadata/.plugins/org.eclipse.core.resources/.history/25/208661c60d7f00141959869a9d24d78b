import java.util.ArrayList;
import java.io.*;
public class Assignment 
{
	int assignmentId = 0;
	int courseId = 0;
	String assignmentName = "";
	int possiblePoints = 0;
	int[] Scores = new int[10];
	
	public int getId() 
	{
		return assignmentId;
	}
	public int getCourse()
	{
		return courseId;
	}
	public String getPoints()
	{
		return possiblePoints;
	}
	
	
}
